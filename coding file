21)Code to find the sum of the numbers
#include<stdio.h>
int main()
{
    int n,sum=0;
    printf("Enter n value\n");
    scanf("%d",&n);
     int i;
    for(i=1;i<=n;i++)
    {
       sum=sum+i;
    }
    printf("\nsum of natural numbers is %d\n",sum);
    return 0;
}

22)Code to find the series of *
#include<stdio.h>
int main()
{
    int i,j,n;
    printf("Enter n value\n");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
       for(j=1;j<=i;j++)
       {
           printf("* ");
       }
       printf("\n");
    }
    return 0;
}

