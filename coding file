        struct browser b1;
        scanf("%s%s%f",b1.name,b1.copyright,&b1.version);
        return b1;
    };

    void display (struct browser b1)
    {
         printf("\n%s\n%s\n%f",bname,bcopyright,n);
    }

    main()
