/*program to compute distance travelled by mahan and sohan*/
#include<stdio.h>
int main()
{
    int x1,y1,x2,y2,distance;
    printf("enter the value of x1,y1,x2,y2\n");
    scanf("%d%d%d%d",&x1,&y1,&x2,&y2);
    distance=(x2-x1)+(y2-y1);
    printf("%d, is the distance travelled");
    return 0;

}

